{
 "$schema": "https://openapi.vercel.sh/vercel.json",
 "version": 2,
 "env": {
    "PORT": "5000"
 },
 "builds": [
   {
     "src": "index.js",
     "use": "@vercel/node"
   }
 ],
 "routes": [
   {
     "src": "/(.*)",
     "dest": "index.js",
     "headers": {
       "Access-Control-Allow-Origin": "*",
       "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
       "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept"
     }
   }
 ]
}